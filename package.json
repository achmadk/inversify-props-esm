{
  "version": "2.0.2",
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "node": "./dist/inversify-props-esm.esm.js",
      "require": "./dist/index.js",
      "import": "./dist/inversify-props-esm.esm.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "dts watch",
    "build": "dts build",
    "test": "dts test --coverage",
    "lint": "dts lint --fix",
    "prepare": "dts build",
    "prepublishOnly": "dts build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "@abraham/reflection": "0.10.0",
    "inversify-esm": "2.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint --fix"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "inversify-props-esm",
  "author": "Achmad Kurnianto",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/achmadk/inversify-props-esm.git"
  },
  "module": "dist/inversify-props-esm.esm.js",
  "size-limit": [
    {
      "path": "dist/inversify-props-esm.cjs.production.min.js",
      "limit": "1.5 KB"
    },
    {
      "path": "dist/inversify-props-esm.esm.js",
      "limit": "15 B"
    }
  ],
  "devDependencies": {
    "@abraham/reflection": "0.10.0",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@size-limit/webpack": "8.1.0",
    "@size-limit/webpack-why": "8.1.0",
    "@skypack/package-check": "0.2.2",
    "@typescript-eslint/eslint-plugin": "5.37.0",
    "@typescript-eslint/parser": "5.37.0",
    "babel-eslint": "10.1.0",
    "dts-cli": "1.6.0",
    "eslint": "8.23.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-flowtype": "8.0.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.31.8",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "^5.0.9",
    "inversify-esm": "2.0.2",
    "prettier": "2.7.1",
    "size-limit": "^8.1.0",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3"
  },
  "keywords": [
    "inversify",
    "typescript",
    "typescript inject",
    "typescript di",
    "react di",
    "vue di",
    "vue inject",
    "react inject",
    "litelement",
    "polymer",
    "support ES Module"
  ],
  "packageManager": "yarn@3.5.1"
}
